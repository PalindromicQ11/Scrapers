Selenium_html.py

#1 imports:
from selenium.driver import webdriver
from selenium.driver.ActionChains import ActionChains
from selenium.support.ui import {select, keys, expected_conditions}
import re

#2 create object and function
global driver = webdriver.firefox()

def set_ScrapeHtml(element:list[int,str], content:list[int,str], *kwarg)--> int,str:
  element = driver.get_element_by_id(r'\W^+[th]{2}[td]{2}[tr]{2}*[h\d]{2}'g)
  content = driver.get_element_by_xpath('//[@class(form = 'inputforms')]')
  output = list(set element & set content)
  return output
  
def set_droppable():
  frame = driver.switch_frame(0)
  element = frame.actionChains.get_element('dropable')
  
